    <script>
        var csrtoken = 'AIzaSyBZmueyxp62ODs8FVuoX2iOv3iGG9EdiDM';
        var _token = '';


        var selectElement = document.getElementById("country_code");

        var selectedOption = selectElement.options[selectElement.selectedIndex];
        var dataPatternSelectedCntr = new RegExp(selectedOption.getAttribute("data-pattern"));
        var dataPatternSelectedCntrNocode = new RegExp(selectedOption.getAttribute("data-pattern_nocode"));
        var is_recover_password = false;

        $(document).ready(function() {
            $('#country_code').select2({
                templateResult: formatCountryFlag,
                templateSelection: formatCountryFlag // Apply to selected item as well
            });
        });

        function formatCountryFlag(country) {
            if (!country.id) {
                return country.text;
            }
            var $country = $(
                '<span><img src="' + country.element.dataset.flag + '" class="img-flag" width="20" /> ' + country.text +
                '</span>'
            );
            return $country;
        }
    </script>

    
